{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.js"
      ]
    },
    {
      "library": "bootstrap@4.5.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "magnific-popup@1.1.0",
      "destination": "wwwroot/lib/magnific-popup/",
      "files": [
        "dist/magnific-popup.css",
        "dist/jquery.magnific-popup.min.js"
      ]
    },
    {
      "library": "owl.carousel@2.3.4",
      "destination": "wwwroot/lib/owl-carousel/",
      "files": [
        "dist/assets/owl.carousel.min.css",
        "dist/owl.carousel.min.js"
      ]
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.min.css"
      ]
    },
    {
      "library": "slicknav@1.0.8",
      "destination": "wwwroot/lib/slicknav/",
      "files": [
        "dist/slicknav.min.css",
        "jquery.slicknav.js"
      ]
    },
    {
      "library": "datatables.net@1.10.25",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js"
      ]
    },
    {
      "library": "datatables.net-dt@1.10.25",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "css/jquery.dataTables.min.css",
        "images/*"
      ]
    },
    {
      "library": "datatables.net-responsive@2.2.9",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/dataTables.responsive.min.js"
      ]
    }
  ]
}
